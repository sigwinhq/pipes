{
    "type": "project",
    "name": "sigwin/pipes",
    "license": "proprietary",
    "description": "Pipes to lead data from source to destination, with filtering and transformation",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "bref/symfony-bridge": "^0.2.0@beta",
        "nyholm/psr7": "^1.4",
        "psr/http-client": "^1.0",
        "symfony/console": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/runtime": "^6.0",
        "symfony/slack-notifier": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "symfony/flex": "^2.0",
        "symfony/maker-bundle": "^1.36"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
